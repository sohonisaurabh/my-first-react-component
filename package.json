{
  "name": "my-first-react-component",
  "version": "1.0.0",
  "description": "Learn to build React Components using TDD",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest --watch",
    "test:unit": "mocha --require babel-core/register --require ./tests-config/helpers.js --require ./tests-config/dom.js \"src/Pagination/Pagination.test.js\"",
    "test:unit:watch": "npm run test:unit -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sohonisaurabh/my-first-react-component.git"
  },
  "keywords": [
    "React",
    "Unit",
    "Testing",
    "TDD",
    "Jest",
    "Enzyme",
    "Mocha",
    "Chai",
    "Pagination"
  ],
  "author": "Saurabh Sohoni",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sohonisaurabh/my-first-react-component/issues"
  },
  "dependencies": {
    "emotion": "^9.2.8",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-emotion": "^9.2.8",
    "react-scripts": "1.1.4"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "setupFiles": [
      "./setupTests.js"
    ]
  },
  "homepage": "https://github.com/sohonisaurabh/my-first-react-component#readme",
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "chai": "^4.1.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.2.0",
    "jsdom": "^11.12.0",
    "mocha": "^5.2.0"
  }
}
